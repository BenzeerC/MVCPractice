@{
    ViewData["Title"] = "Index";
}
<h1>List</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Marks</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Student s in (IEnumerable<Student>)ViewData["stu"])
        {
            <tr>
                <th>@s.Id</th>
                <th>@s.Name</th>
                <th>@s.Marks</th>

            </tr>
        }
    </tbody>
</table>
